    "mobx-react-lite": "^0.3.4",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router-dom": "^4.3.1",
    "react-use": "^5.2.1",
    "typestyle": "^2.0.1"
